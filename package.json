{
  "name": "ts-node-template",
  "version": "0.0.1",
  "scripts": {
    "format": "prettier --write .",
    "dev": "nodemon --config nodemon.json src/index.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/index.ts",
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc",
    "start": "node build/index.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.14",
    "dotenv": "^15.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
